<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="come.jobfinder.dev"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="14" />

 	<uses-permission android:name="android.permission.READ_CALENDAR" />
    <uses-permission android:name="android.permission.WRITE_CALENDAR" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name=".JobFinderActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MyMenu"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="come.jobfinder.dev.MENU" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddWorkPlace"
            android:label="@string/app_name" >
              <intent-filter>
                <action android:name="come.jobfinder.dev.ADDWORKPLACE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Prefs"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="come.jobfinder.dev.PREFS" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AboutUss"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Dialog" >
            <intent-filter>
                <action android:name="come.jobfinder.dev.ABOUT" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
         android:name = ".EmailCompany"   
         android:label = "@string/app_name">  
        </activity>
        <activity
         android:name = ".ListOfWorkPlaces"
         android:label = "@string/app_name">
        </activity>
        <activity 
         android:name = ".UpdateWorkPlace"
         android:label = "@string/app_name"   
            ></activity>
    </application>

</manifest>